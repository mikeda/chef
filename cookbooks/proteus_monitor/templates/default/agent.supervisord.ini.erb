[program:proteus-monitor-agent]
command=/home/sysadmin/proteus_monitor/node/bin/node /home/sysadmin/proteus_monitor/agent/agent.js --config <%= node.proteus_monitor.agent.config %>
process_name=%(program_name)s
stdout_logfile_maxbytes=1MB
stderr_logfile_maxbytes=1MB
stdout_logfile=/var/log/supervisor/%(program_name)s.log
stderr_logfile=/var/log/supervisor/%(program_name)s.log
user=sysadmin
autostart=true
autorestart=true
